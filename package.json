{
  "name": "probot-require-label",
  "version": "0.1.1",
  "description": "A Probot that applies a label to issues and/or PRs that do not have any labels matching a regular expression",
  "main": "lib/index.js",
  "scripts": {
    "test": "rm -rf lib && jest",
    "build": "rm -rf lib && tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "start": "probot run ./lib/index.js",
    "dev": "LOG_LEVEL=debug nodemon --exec \"npm start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lswith/probot-require-label.git"
  },
  "engines": {
    "node": 12
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "probot"
  ],
  "author": "Luke Swithenbank",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lswith/probot-require-label/issues"
  },
  "homepage": "https://github.com/lswith/probot-require-label#readme",
  "dependencies": {
    "joi": "^14.3.1",
    "probot": "^9.13.0",
    "probot-config-manager": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/travis-cli": "^9.1.2",
    "@types/jest": "^26.0.0",
    "@types/joi": "^14.3.4",
    "@types/node": "^12.12.54",
    "husky": "^4.2.5",
    "jest": "^26.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.0",
    "semantic-release": "^17.0.0",
    "smee-client": "^1.2.2",
    "ts-jest": "^26.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
